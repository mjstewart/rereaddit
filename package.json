{
  "name": "reddit-extension-babel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && cross-env NODE_ENV=test mocha-webpack --colors --webpack-config config/webpack.config.ts  ./src/**/*-test.*",
    "test:watch": "yarn run clean && cross-env NODE_ENV=test mocha-webpack --watch --colors --webpack-config config/webpack.config.ts  ./src/**/*-test.*",
    "build:dev": "yarn run clean && webpack --config config/webpack.config.ts --display-error-details --progress --colors",
    "build:prod": "yarn run clean && webpack -p --config config/webpack.config.production.ts --display-error-details --progress --colors",
    "start": "webpack-dev-server --config config/webpack.config.ts",
    "lint": "echo \"Error: no linting specified\" && exit 1",
    "clean": "rm -rf dist && rm -rf .tmp"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chrome": "^0.0.49",
    "@types/mocha": "^2.2.43",
    "@types/sinon-chrome": "^0.2.28",
    "@types/webpack": "^3.0.13",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "mocha-webpack": "next",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "sinon-chrome": "^2.2.1",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "lodash.has": "^4.5.2",
    "lodash.isempty": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.19.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2"
  }
}
